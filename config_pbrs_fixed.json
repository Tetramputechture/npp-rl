{
  "experiment_name": "mlp-pbrs-fixed",
  "architectures": [
    "mlp_baseline"
  ],
  "train_dataset": "../nclone/datasets/train",
  "test_dataset": "../nclone/datasets/test",
  "output_dir": "./experiments",
  "test_pretraining": false,
  "no_pretraining": false,
  "replay_data_dir": "../nclone/bc_replays",
  "bc_epochs": 20,
  "bc_batch_size": 128,
  "total_timesteps": 5000000,
  "num_envs": 128,
  "eval_freq": 250000,
  "save_freq": 1000000,
  "num_eval_episodes": 5,
  "skip_final_eval": false,
  "hardware_profile": "auto",
  "num_gpus": 1,
  "distributed_backend": "nccl",
  "mixed_precision": true,
  "use_hierarchical_ppo": false,
  "high_level_update_freq": 50,
  "use_curriculum": true,
  "curriculum_start_stage": "simplest",
  "curriculum_threshold": 0.4,
  "curriculum_min_episodes": 100,
  "enable_pbrs": true,
  "pbrs_gamma": 0.995,
  "enable_mine_avoidance_reward": true,
  "disable_trend_advancement": false,
  "disable_early_advancement": false,
  "enable_lr_annealing": true,
  "initial_lr": 0.0003,
  "enable_visual_frame_stacking": false,
  "visual_stack_size": 1,
  "enable_state_stacking": false,
  "state_stack_size": 4,
  "frame_stack_padding": "zero",
  "record_eval_videos": true,
  "max_videos_per_category": 2,
  "video_fps": 60,
  "resume_from": null,
  "visualize_training": false,
  "vis_render_freq": 100,
  "vis_env_idx": 0,
  "vis_fps": 60,
  "debug": false,
  "batch_size": 512,
  "n_steps": 2048,
  "learning_rate": 0.0003
}